<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>indexBean</managed-bean-name>
		<managed-bean-class>beans.indexBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>article</property-name>
			<property-class>java.util.List</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>beans.userBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>password</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>username</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>logoutbean</managed-bean-name>
		<managed-bean-class>beans.logoutbean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>supprimerBean</managed-bean-name>
		<managed-bean-class>beans.supprimerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>codearticle</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>approvisoinnementBean</managed-bean-name>
		<managed-bean-class>beans.approvisoinnementBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>approvi</property-name>
			<property-class>java.util.List</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>codeArt</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>qteCommande</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>dateLivraison</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ajouterArticleBean</managed-bean-name>
		<managed-bean-class>beans.ajouterArticleBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>nomarticle</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>descarticle</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>qntarticle</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>prixarticle</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>modifierBean</managed-bean-name>
		<managed-bean-class>beans.modifierBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>codeArt</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>descArt</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>formulaireModification</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>nomArt</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>prixArt</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>qntArt</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>Modifier</from-outcome>
			<to-view-id>/modifier.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>Supprimer</from-outcome>
			<to-view-id>/SupprimerArticle.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>Ajouter</from-outcome>
			<to-view-id>/AjouterArticle.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>Approvisoinement</from-outcome>
			<to-view-id>/Approvisoinement.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<lifecycle>
   		<phase-listener>util.LoggedInCheck</phase-listener>
	</lifecycle>
</faces-config>
